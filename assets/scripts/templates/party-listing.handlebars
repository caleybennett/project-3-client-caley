{{#each party as |party|}}
  <section data-id="{{party._id}}" class="party-log" >
    <h6>Date:</h6>
    <p>
      {{party.date}}
    </p>
    <h6>Title:</h6>
    <p>
        {{party.title}}
    </p>
    <h6>Details:</h6>
    <p>
        {{party.details}}
    </p>

    <button data-id="{{party._id}}" class="btn btn-danger rsvp-btn"> RSVP </button>
    </section>

  {{/each}}
