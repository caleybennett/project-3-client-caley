{{#each rsvps as |rsvp|}}
{{log rsvp.party}}
  <section data-id="{{rsvp.party}}" class="party-log" >
    <h4>
      {{rsvp._id}}
    </h4>
    <h6>Date:</h6>
    <h4>
      {{rsvp.party.date}}
    </h4>
    <h6>Title:</h6>
    <p>
        {{rsvp.party.title}}
    </p>
    <h6>Details:</h6>
    <p>
        {{rsvp.party.details}}
    </p>
  </section>

{{/each}}
